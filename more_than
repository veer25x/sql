CREATE TABLE students (
    id INT PRIMARY KEY,
    name VARCHAR(50),
    age INT,
    course VARCHAR(50),
    marks INT
);

INSERT INTO students VALUES (1, 'Veer', 20, 'AI', 85);
INSERT INTO students VALUES (2, 'uttu', 21, 'ML', 90);
INSERT INTO students VALUES (3, 'anni', 19, 'DS', 88);

SELECT * FROM students;

SELECT name, marks FROM students;

SELECT * FROM students WHERE marks > 85;

UPDATE students
SET marks = 95
WHERE id = 1;
